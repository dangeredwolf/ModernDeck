img.spinner-centered {
  display:block!important;
  box-sizing: border-box!important;
  width: 200px!important;
  height: 200px!important;
  background: url(https://ryandolan123.com/assets/tweetdeck/img/TweetDeckWhite.png) no-repeat!important;
  margin-left: -100px!important;
  margin-top: -100px!important;
  padding-left: 200px;
}

.scroll-v .js-startflow-content.startflow {
  /* background-image: none; */
}

.js-startflow-content.startflow {
  background: url(https://ryandolan123.com/tweetdeck-web/web/assets/global/backgrounds/spinner-fff-on-55acee.gif) no-repeat;
    height: 20px;
    width: 20px;
    top: 0px;
    margin-top: 550px;
    margin-bottom: 0px;
    display: block;
    content: " ";
    background-size: 20px;
    left: 50%;
    padding-left: 50%;
    margin-right: -8px;
    /*position: absolute;*/
    float: right;
    right: 50%;
}

