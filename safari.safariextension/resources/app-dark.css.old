.btn {
  box-sizing:border-box;
  border-radius:4px;
  position:relative;
  display:inline-block;
  margin:0;
  padding:4px 10px;
  min-height:30px;
  vertical-align:middle;
  text-align:center;
  cursor:pointer;
  text-decoration:none;
  font-size:14px;
  font-weight:400;
  border:0px solid #ddd;
  color: #fff;
  background-color: #555;
  text-shadow:none;
  transition:200ms all!important;
  transform: scale(1,1)
}

